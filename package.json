    "lint:eslint": "eslint . --cache --fix",
    "lint:stylelint": "stylelint \"src/**/*.{css,scss,vue}\" --cache --fix",
    "postinstall": "simple-git-hooks",
    "preinstall": "npx only-allow pnpm",
    "commit": "git cz",
    "release": "bumpp"
